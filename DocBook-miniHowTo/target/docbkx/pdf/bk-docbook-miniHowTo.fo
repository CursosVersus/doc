	</fo:table-row></fo:table-header><fo:table-body start-indent="0pt" end-indent="0pt"><fo:table-row>
	  <fo:table-cell padding-start="2pt" padding-end="2pt" padding-top="2pt" padding-bottom="2pt" border-start-style="none" border-top-style="none" border-bottom-style="solid" border-bottom-width="0.5pt" border-bottom-color="black" border-end-style="solid" border-end-width="0.5pt" border-end-color="black"><fo:block>Fila 1 - columna 1</fo:block></fo:table-cell>
